{
  "name": "Octokit.rb flavored GitHub API Docs",
  "short_name": "octodoctotron",
  "description": "Add Octokit.rb method names and documentation links to the offical GitHub API docs on developer.github.com.",
  "homepage_url": "https://github.com/joeyw/octodoctotron",
  "manifest_version": 2,
  "version": "0.1.0",
  "content_scripts": [
    {
      "matches": [
        "http://developer.github.com/*",
        "https://developer.github.com/*"
      ],
      "js": [
        "jquery.js",
        "inject.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "jquery.js",
    "inject.js"
  ]
}
